{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },

    "Serilog": {
      "WriteTo": [
        { "Name": "File", "Args": { "path": "Cryptolog.txt", "rollingInterval": "Day" } }
      ]
    }
  },
  "AllowedHosts": "*"
}