@using Crypto.Core.Models

@{var index = 0;}

@model IEnumerable<CurrencyTest>

<table>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>@(++index)</td>
                <td>@item.Currency</td>
                <td>@item.Prices.Last()</td>
            </tr>
            
        }
    </tbody>
</table>